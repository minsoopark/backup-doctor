<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="sgen.backup.dr"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="17"/>
    <application
            android:label="@string/app_name"
            android:icon="@drawable/ic_launcher"
            android:theme="@style/AppBaseWhiteTheme">
        <activity android:name=".activities.SplashActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity"/>
        <activity android:name=".activities.JoinActivity"/>
        <activity android:name=".activities.PolicyActivity"/>
        <activity android:name=".activities.DetailInputActivity"/>
        <activity android:name=".activities.MainActivity"/>
        <activity android:name=".activities.SymptomActivity"/>
        <activity android:name=".activities.TestResultActivity"/>
        <activity android:name=".activities.HistoryActivity"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
